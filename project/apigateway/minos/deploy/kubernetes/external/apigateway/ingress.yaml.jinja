apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: minos-apigateway
  namespace: {{ project_name }}
  annotations:
    kubernetes.io/ingress.class: nginx
spec:
  rules:
    - http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: minos-apigateway
                port:
                  number: 5566